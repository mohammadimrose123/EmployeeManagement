
@model IEnumerable<EmployeeManagement.Models.Employee>
<h2>Employee List</h2>
<a href="/Employee/Create">Add Employee</a>
<table>
<tr><th>Name</th><th>Dept</th><th>Email</th><th>Salary</th><th>Actions</th></tr>
@foreach(var e in Model){
<tr>
<td>@e.Name</td><td>@e.Department</td><td>@e.Email</td><td>@e.Salary</td>
<td><a href="/Employee/Edit/@e.Id">Edit</a> | <a href="/Employee/Delete/@e.Id">Delete</a></td>
</tr>}
</table>
